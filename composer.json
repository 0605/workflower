{
    "name": "phpmentors/workflower",
    "type": "library",
    "description": "A workflow engine",
    "keywords": ["flow", "workflow", "process", "bpm", "bpmn"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "phpmentors/domain-kata": "~1.4@dev",
        "piece/stagehand-fsm": "~2.4@dev",
        "symfony/expression-language": "2.6.*"
    },
    "require-dev": {
        "phake/phake": "~2.0",
        "phpunit/phpunit": "~4.0",
        "piece/stagehand-testrunner": "~4.1@dev"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\Workflower\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPMentors\\Workflower\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
